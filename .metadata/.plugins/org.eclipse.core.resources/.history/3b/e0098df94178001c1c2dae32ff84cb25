package com.simformsolutions.practicals.thread;

public class ThreadExtend extends Thread {
		
	int counter=0;
	@Override
    public void run()
    {
		
        try {
        	counter++;
            System.out.println("Counter IS "+counter);
        }
        catch (Exception e) {
            System.out.println("Exception is caught");
        }
    }
}

