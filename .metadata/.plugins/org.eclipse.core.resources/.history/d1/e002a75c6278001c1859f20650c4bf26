package com.simformsolutions.practicals.thread;

//Driver Class Which Invokes Threads
public class Main {

	public static void main(String[] args) {

		ThreadExtend thOne = new ThreadExtend();
		ThreadExtend thTwo = new ThreadExtend();
		ThreadExtend thThree = new ThreadExtend();
		ThreadExtend thFour = new ThreadExtend();
		
		System.out.println("Name     ID Priority");
		thThree.setPriority(10);
		
		thOne.start();
		thTwo.start();
		thThree.start();
		thFour.start();
		
	}

}

